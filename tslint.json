{
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "no-redundant-jsdoc": true,
    "file-name-casing": [
      true,
      "kebab-case"
    ],
    "newline-before-return": true,
    "jsx-no-lambda": false,
    "no-empty-interface": false,
    "member-access": [true, "no-public"]
  }
}
